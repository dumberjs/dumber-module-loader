{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": [2, "always"],
    "no-undef": 2,
    "no-prototype-builtins": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}

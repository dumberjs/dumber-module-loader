{
  "name": "dumber-module-loader",
  "version": "0.15.15",
  "description": "A modern module loader (loose AMD implementation), designed to work with dumber.",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=development rollup -c",
    "lint": "eslint src test",
    "prepare": "npm run build",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "nodejs-test": "cross-env BABEL_ENV=test tape -r @babel/register \"test/**/*.spec.js\" | tap-dot",
    "browser-test": "cross-env BABEL_ENV=test browserify -t babelify test/all-spec.js | tape-run --static . | tap-dot",
    "amdjs-test": "npm run build && node amdjs-run",
    "amdjs-browser": "npm run build && browser-run --input html --mock amdjs-tests/mock.js --browser chrome < amdjs-tests/index.html",
    "coverage": "cross-env BABEL_ENV=test nyc --reporter=lcov tape -r @babel/register \"test/**/*.spec.js\" | tap-dot",
    "test": "npm run nodejs-test && npm run browser-test && npm run amdjs-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber-module-loader"
  },
  "keywords": [
    "dumber",
    "amd",
    "requirejs"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber-module-loader/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber-module-loader#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browser-run": "^5.0.1",
    "browserify": "^16.2.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "express": "^4.16.4",
    "nyc": "^13.1.0",
    "rollup": "^1.0.2",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-terser": "^4.0.1",
    "standard-changelog": "^2.0.6",
    "tap-dot": "^2.0.0",
    "tape": "^4.9.2",
    "tape-run": "^5.0.0"
  },
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "dependencies": {}
}

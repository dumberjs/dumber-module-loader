{
  "name": "dumber-module-loader",
  "version": "0.16.1",
  "description": "A modern module loader (loose AMD implementation), designed to work with dumber.",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=development rollup -c",
    "lint": "eslint src test",
    "prepare": "npm run build",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "nodejs-test": "cross-env BABEL_ENV=test tape -r @babel/register \"test/**/*.spec.js\" | tap-dot",
    "browser-test": "cross-env BABEL_ENV=test browserify -t babelify test/all-spec.js | browser-do --tap --static . | tap-dot",
    "amdjs-test": "npm run build && node amdjs-run",
    "amdjs-browser": "npm run build && browser-do --mock amdjs-tests/mock.js --browser chrome < amdjs-tests/index.html",
    "coverage": "cross-env BABEL_ENV=test nyc --reporter=lcov tape -r @babel/register \"test/**/*.spec.js\" | tap-dot",
    "test": "npm run nodejs-test && npm run browser-test && npm run amdjs-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber-module-loader"
  },
  "keywords": [
    "dumber",
    "amd",
    "requirejs"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber-module-loader/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber-module-loader#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "nyc": "^14.1.1",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "standard-changelog": "^2.0.11",
    "tap-dot": "^2.0.0",
    "tape": "^4.10.2",
    "browser-do": "^0.2.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}

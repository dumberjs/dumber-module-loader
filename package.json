{
  "name": "dumber-module-loader",
  "version": "0.13.0",
  "description": "A modern module loader (loose AMD implementation), designed to work with dumber.",
  "main": "dist/index.js",
  "scripts": {
    "build": "env BABEL_ENV=development rollup -c",
    "lint": "eslint src",
    "prepare": "npm run build",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "nodejs-test": "env BABEL_ENV=test tape -r @babel/register 'test/**/*.spec.js' | tap-dot",
    "browser-test": "env BABEL_ENV=test browserify -t babelify test/all-spec.js | tape-run --static . | tap-dot",
    "amdjs-test": "node amdjs-run",
    "test": "npm run nodejs-test && npm run browser-test && npm run amdjs-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dumberjs/dumber-module-loader"
  },
  "keywords": [
    "dumber",
    "amd",
    "requirejs"
  ],
  "author": "Chunpeng Huo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dumberjs/dumber-module-loader/issues"
  },
  "homepage": "https://github.com/dumberjs/dumber-module-loader#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babelify": "^10.0.0",
    "browser-run": "^5.0.0",
    "browserify": "^16.2.3",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "standard-changelog": "^2.0.6",
    "tap-dot": "^2.0.0",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0"
  },
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "dependencies": {}
}
